[*]
charset = utf-8
end_of_line = crlf
insert_final_newline = false
indent_style = tab
indent_size = 4
tab_width = 4

[{.babelrc, .stylelintrc, .eslintrc, jest.config, *.bowerrc, *.jsb3, *.jsb2, *.json}]
indent_style = space
indent_size = 2

[{.analysis_options, *.yml, *.yaml}]
indent_style = space
indent_size = 2

[*.dart]
indent_style = tab
tab_width = 4
indent_size = 4

